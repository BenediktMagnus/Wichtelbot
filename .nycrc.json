{
    "cache": false,
    "all": true,
    "reporter": [
        "html",
        "text"
    ],
    "require": [
        "ts-node/register/transpile-only"
    ],
    "extension": [
        ".ts"
    ],
    "include": [
        "scripts/**/*.ts"
    ],
    "exclude": [
        "**/*.d.ts",
        "coverage/**",
        "packages/*/test/**",
        "test/**",
        "tests/**",
        "test{,-*}.ts",
        "tests{,-*}.ts",
        "**/*{.,-}{test,spec}.ts",
        "**/__tests__/**",
        "**/node_modules/**"
    ],
    "temp-directory": "./coverage/.nyc_output"
}
